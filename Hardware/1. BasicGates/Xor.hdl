/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a, out=NotA);
	Not(in=b, out=NotB);
	And(a=a, b=NotB, out=ATrue);
	And(a=NotA, b=b, out=BTrue);
	Or(a=ATrue, b=BTrue, out=out);
}